{
  "name": "angel-task-mgmt",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node dist/index.js",
    "predev": "npm run swagger",
    "prebuild": "npm run swagger",
    "build": "tsc --build",
    "dev": "concurrently \"nodemon\" \"nodemon -x tsoa spec\"",
    "swagger": "tsoa spec",
    "clean": "tsc --build --clean",
    "test": "echo \"Error: no test specified\" && exit 1",
    "typeorm": "typeorm-ts-node-commonjs -d ./src/config/data-source.ts",
    "db:history": "ts-node node_modules/.bin/typeorm -d ./src/config/data-source.ts schema:sync",
    "db:generate-migration": "ts-node node_modules/.bin/typeorm -d ./src/config/data-source.ts migration:generate ./src/database/migrations/${npm_config_name} --",
    "db:migrate": "ts-node node_modules/.bin/typeorm -d ./src/config/data-source.ts migration:run",
    "db:rollback": "ts-node node_modules/.bin/typeorm -d ./src/config/data-source.ts migration:revert",
    "lint": "eslint . --ext .ts"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts",
    "exec": "ts-node src/index.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RIZE-Inc/angel-task-mgmt.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/RIZE-Inc/angel-task-mgmt/issues"
  },
  "homepage": "https://github.com/RIZE-Inc/angel-task-mgmt#readme",
  "devDependencies": {
    "@types/express": "^4.17.14",
    "@types/moment-timezone": "^0.5.30",
    "@types/morgan": "^1.9.3",
    "@types/node": "^18.8.5",
    "@types/node-fetch": "^2.6.2",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^5.40.1",
    "@typescript-eslint/parser": "^5.40.1",
    "concurrently": "^7.4.0",
    "eslint": "^8.25.0",
    "nodemon": "^2.0.20",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "@line/bot-sdk": "^7.5.2",
    "@slack/oauth": "^2.5.4",
    "@slack/socket-mode": "^1.3.2",
    "@slack/web-api": "^6.8.0",
    "@slack/webhook": "^6.1.0",
    "@notionhq/client": "^2.2.2",
    "@types/cors": "^2.8.12",
    "body-parser": "^1.20.1",
    "class-sanitizer": "^1.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "dotenv-expand": "^9.0.0",
    "express": "^4.18.2",
    "form-data": "^4.0.0",
    "moment": "^2.29.4",
    "moment-timezone": "0.5.34",
    "morgan": "^1.10.0",
    "mysql": "^2.18.1",
    "node-fetch": "^2.6.6",
    "tsoa": "^4.1.3",
    "typedi": "^0.10.0",
    "typeorm": "^0.3.10",
    "winston": "^3.8.2"
  }
}
