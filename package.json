{
  "name": "angel-task-mgmt",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node dist/index.js",
    "build": "tsc --build && tsc-alias -p tsconfig.json",
    "dev": "export NODE_HTTPS=false && ts-node-dev -r tsconfig-paths/register src/index.ts",
    "dev:https": "export NODE_HTTPS=true && ts-node-dev -r tsconfig-paths/register src/index.ts",
    "swagger": "tsoa spec",
    "clean": "tsc --build --clean",
    "test": "echo \"Error: no test specified\" && exit 1",
    "typeorm": "typeorm-ts-node-commonjs -d ./src/config/data-source.ts",
    "db:history": "export DB_HOST='127.0.0.1' && ts-node node_modules/.bin/typeorm -d ./src/config/data-source.ts schema:sync",
    "db:create-migration": "export DB_HOST='127.0.0.1' && ts-node node_modules/.bin/typeorm migration:create ./src/database/migrations/${npm_config_name} --",
    "db:generate-migration": "export DB_HOST='127.0.0.1' && ts-node node_modules/.bin/typeorm -d ./src/config/data-source.ts migration:generate ./src/migrations/${npm_config_name} --",
    "db:migrate": "export DB_HOST='127.0.0.1' && ts-node node_modules/.bin/typeorm -d ./src/config/data-source.ts migration:run",
    "db:rollback": "export DB_HOST='127.0.0.1' && ts-node node_modules/.bin/typeorm -d ./src/config/data-source.ts migration:revert",
    "lint": "eslint . --ext .ts",
    "lintfix": "eslint . --fix --ext .ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RIZE-Inc/angel-task-mgmt.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/RIZE-Inc/angel-task-mgmt/issues"
  },
  "homepage": "https://github.com/RIZE-Inc/angel-task-mgmt#readme",
  "devDependencies": {
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.14",
    "@types/express-session": "^1.17.6",
    "@types/moment-timezone": "^0.5.30",
    "@types/morgan": "^1.9.3",
    "@types/node": "^18.8.5",
    "@types/node-fetch": "^2.6.2",
    "@typescript-eslint/eslint-plugin": "^5.40.1",
    "@typescript-eslint/parser": "^5.40.1",
    "eslint": "^8",
    "eslint-import-resolver-alias": "^1.1.2",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.8.2",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^5.0.2",
    "typescript-transform-paths": "^3.4.6"
  },
  "dependencies": {
    "@line/bot-sdk": "^7.5.2",
    "@notionhq/client": "^2.2.3",
    "@slack/oauth": "^2.6.0",
    "@slack/web-api": "^6.8.0",
    "@slack/webhook": "^6.1.0",
    "axios": "^1.5.1",
    "backlog-js": "^0.12.6",
    "class-sanitizer": "^1.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "connect-typeorm": "^2.0.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "date-holidays": "^3.16.17",
    "dayjs": "^1.11.7",
    "dotenv": "^16.0.3",
    "dotenv-expand": "^10.0.0",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "firebase-admin": "^11.5.0",
    "form-data": "^4.0.0",
    "isomorphic-fetch": "^3.0.0",
    "isomorphic-form-data": "^2.0.0",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.29.4",
    "moment-timezone": "0.5.41",
    "morgan": "^1.10.0",
    "mysql": "^2.18.1",
    "node-fetch": "^2.6.6",
    "tsoa": "^5.1.1",
    "typedi": "^0.10.0",
    "typeorm": "^0.3.12",
    "winston": "^3.8.2"
  }
}
